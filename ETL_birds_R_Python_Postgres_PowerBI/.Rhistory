source("G:/Research data/Other groups/Ebird_data/ETL_birds_R_Python_Postgres_PowerBI/ebird_api.R")
library(rebird)
library(tidyverse)
date_seq<-seq(as.Date("2022-01-01"),
as.Date("2022-07-31"),
1)
date_seq
data_bird_fn<-function(x) {
step_1 = ebirdhistorical(loc = "IN-MH-PU",
date = x,
fieldSet = 'full',
key=api_ebird)
return(step_1)
}
bird_data<-lapply(date_seq,data_bird_fn)%>%
bind_rows()
write.csv(bird_data,'G:/Research data/Other groups/Ebird_data/bird_data_extraction.csv')
gc()
